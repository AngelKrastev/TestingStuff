package project_1;
import java.util.*;

public class excercise_3 {
	
	private static Scanner scanner;
	
	public static void main(String args[]) {
		scanner = new Scanner( System.in );
		int n=scanner.nextInt();
		for(int i=0;i<n;i++){
			for(int j=0;j<n;j++) System.out.print("*");
			System.out.println();
		}
		
	}
}
